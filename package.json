{
  "name": "@rcsv/core",
  "version": "0.1.0-poc",
  "description": "Rich CSV (RCSV) parser and calculation engine - Markdown for spreadsheets",
  "main": "./dist/core/index.js",
  "module": "./dist/core/index.mjs",
  "types": "./dist/core/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/core/index.d.ts",
      "import": "./dist/core/index.mjs",
      "require": "./dist/core/index.js"
    },
    "./renderer": {
      "types": "./dist/renderer/index.d.ts",
      "import": "./dist/renderer/index.mjs",
      "require": "./dist/renderer/index.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "npm run build:parser && tsc && vite build",
    "build:parser": "npm run build:peggy && npm run fix:exports",
    "build:peggy": "peggy --allowed-start-rules Formula --format es --output src/core/parser/formula.ts src/core/parser/formula.pegjs",
    "fix:exports": "node scripts/fix-parser-exports.js",
    "preview": "vite preview",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "csv",
    "rcsv",
    "spreadsheet",
    "parser",
    "formula",
    "calculation",
    "markdown",
    "tabular",
    "data"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/papaparse": "^5.3.14",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vitest/ui": "^1.0.0",
    "chart.js": "^4.5.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "happy-dom": "^18.0.1",
    "peggy": "^4.0.3",
    "prettier": "^3.1.0",
    "typescript": "^5.3.2",
    "vite": "^5.0.7",
    "vitest": "^1.0.0"
  },
  "dependencies": {
    "csv-parse": "^6.1.0",
    "date-fns": "^3.6.0",
    "papaparse": "^5.4.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/evillrich/rcsv-core.git"
  },
  "bugs": {
    "url": "https://github.com/evillrich/rcsv-core/issues"
  },
  "homepage": "https://github.com/evillrich/rcsv-core#readme"
}
